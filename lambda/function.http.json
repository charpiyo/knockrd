{
    "Description": "Example of knockrd-http",
    "Environment": {
        "Variables": {
            "KNOCKRD_DEBUG": "{{ env `DEBUG` `true`}}"
        }
    },
    "FunctionName": "knockrd-http",
    "Handler": "knockrd",
    "MemorySize": 128,
    "Role": "arn:aws:iam::{{ must_env `AWS_ACCOUNT_ID` }}:role/knockrd_lambda",
    "Runtime": "go1.x",
    "Tags": {},
    "Timeout": 5,
    "TracingConfig": {
        "Mode": "PassThrough"
    }
}
